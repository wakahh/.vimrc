syntax on
set number
set title
set ambiwidth=double
set tabstop=2
set expandtab
set shiftwidth=2
set smartindent
set list
set listchars=tab:»-,trail:-,extends:»,precedes:«,nbsp:%
set nrformats-=octal
set hidden
set history=50
set virtualedit=block
set whichwrap=b,s,[,],<,>
set backspace=indent,eol,start
set wildmenu
set background=dark
colorscheme hybrid
imap [ []<left>
imap ( ()<left>
imap { {}<left>
set clipboard+=unnamed
imap <C-f> <right>
imap <C-b> <left>
